FIX(@LIST(@ILDESCENDANTS({СТАТЬИ}),@RDESCENDANTS({СТАТЬИ})) and @Relative("СТАТЬИ", 0))
FIX(@LIST(@ILDESCENDANTS({СЧЕТА_БАЛАНСА}),@RDESCENDANTS({СЧЕТА_БАЛАНСА})) and @Relative("СЧЕТА БАЛАНСА", 0))
FIX(@ILDESCENDANTS({МВЗ_ПОЛУЧАТЕЛЬ}) and @Relative("Итого по МВЗ (пол-ль)", 0))
FIX({ПОКАЗАТЕЛИ})
