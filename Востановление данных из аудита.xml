      @CONCATENATE(@CONCATENATE(@NAME({ВЕРСИЯ}),@NAME({ГОД})),@CONCATENATE(@NAME({ВАЛЮТА}),@NAME(@CURRMBR("ВИД_ЗАТРАТ"))))),
      @CONCATENATE(@CONCATENATE(@CONCATENATE(@NAME(@CURRMBR("КОНТРАГЕНТЫ")),@NAME(@CURRMBR("МВЗ"))),@CONCATENATE(@NAME(@CURRMBR("НАПРАВЛЕНИЯ")),@NAME({ПОКАЗАТЕЛИ}))),
      @CONCATENATE(@CONCATENATE(@NAME(@CURRMBR("ПРОДУКЦИЯ")),@NAME(@CURRMBR("ПРОЧЕЕ"))),@CONCATENATE(@NAME(@CURRMBR("СТАТЬИ")),@NAME(@CURRMBR("ЭЛЕМЕНТЫ ЗАТРАТ")))))),
      @NAME(@CURRMBR("МВЗ ПОЛУЧАТЕЛЬ"))))
      )!=0)*/
     
  IF(@ISMBR(@Relative("Итого по видам затрат", 0)))     

@XWRITE(@StringtoDouble(@HypDataBackToAudit(
        @CONCATENATE(
        @CONCATENATE(
        @CONCATENATE(
        @CONCATENATE(@CONCATENATE(@NAME({СЦЕНАРИЙ}),@NAME(@CURRMBR("СЧЕТА БАЛАНСА"))),@CONCATENATE(@NAME(@CURRMBR("ЦФО")),@NAME(@CURRMBR("ПЕРИОД")))),
        @CONCATENATE(@CONCATENATE(@NAME({ВЕРСИЯ}),@NAME({ГОД})),@CONCATENATE(@NAME({ВАЛЮТА}),@NAME(@CURRMBR("ВИД_ЗАТРАТ"))))),
        @CONCATENATE(@CONCATENATE(@CONCATENATE(@NAME(@CURRMBR("КОНТРАГЕНТЫ")),@NAME(@CURRMBR("МВЗ"))),@CONCATENATE(@NAME(@CURRMBR("НАПРАВЛЕНИЯ")),@NAME({ПОКАЗАТЕЛИ}))),
        @CONCATENATE(@CONCATENATE(@NAME(@CURRMBR("ПРОДУКЦИЯ")),@NAME(@CURRMBR("ПРОЧЕЕ"))),@CONCATENATE(@NAME(@CURRMBR("СТАТЬИ")),@NAME(@CURRMBR("ЭЛЕМЕНТЫ ЗАТРАТ")))))),
        @NAME(@CURRMBR("МВЗ ПОЛУЧАТЕЛЬ"))))),@LOOPBACK,{ГОД});
     
    
        
     
  ENDIF
ENDIF )

SET CREATENONMISSINGBLK OFF;

{ГОД}(
   IF({ГОД}==0);
       #missing;
       ENDIF;)
