{ГОД}(
   IF(@LEV({СЦЕНАРИЙ}) +  @LEV({ВЕРСИЯ}) + @LEV({ГОД})+ @LEV({ПОКАЗАТЕЛИ})+ @LEV({ВАЛЮТА}) >0)
      @RETURN ("Выбран не допустимый срез для востановления.",ERROR);

 ELSE/*IF(
      @CalcMgrTextLength(
      @HypDataBackToAudit(
      @CONCATENATE(
