      <script><![CDATA[/*Вставить продукт на форму 3.3*/
FIX(
{ГОД}
{ВАЛЮТА}
{СЦЕНАРИЙ}
{ВЕРСИЯ}
@RELATIVE("Всего год",0)
@RELATIVE({ВИД_ЗАТРАТ},0) and @LIST("МПЗ собственного производства в целом","Материалы на ремонт","Ремонт силами подрядчиков","Рем.работы ЦРМО","Рем.работы ТЭРЦ","Рем. работы уч. ремонта метал-ого обор-я","Рем. работы уч. ремонтно-строительных работ","Рем. работы уч. ремонта ДП-1","Рем. работы Электроцеха","МПЗ собственного производства в целом","Материалы на ремонт","Ремонт силами подрядчиков","Рем.работы ЦРМО","Рем.работы ТЭРЦ","Рем. работы уч. ремонта метал-ого обор-я","Рем. работы уч. ремонтно-строительных работ","Рем. работы уч. ремонта ДП-1","Рем. работы Электроцеха","Инвентарь","Тара и тарные материалы","Запчасти","Прочие материалы","МПЗ собственного производства в целом","ТО силами подрядчиков","Сменное оборудование","Сменный инструмент")
@RELATIVE({МВЗ},0)
@RELATIVE({КОНТРАГЕНТЫ},0)
@RELATIVE({НАПРАВЛЕНИЯ},0)
@ILDESCENDANTS({СПИСОК_ПРИОДУКЦИЯ})and @RELATIVE("Итого по продукции",0) 
"Здания"
@RELATIVE({ЦФО},0)
@RELATIVE({ЭЛЕМЕНТЫ_ЗАТРАТ},0)
@RELATIVE({СТАТЬИ},0) and @LIST("Некапитализируемые капитальные ремонты","Текущие ремонты","Техническое обслуживание.","Сменный инструмент и оборудование")
@RELATIVE({СЧЕТА_БАЛАНСА},0)
@RELATIVE({МВЗ_ПОЛУЧАТЕЛЬ},0) and @LIST("бу МВЗ (пол-ль)","Выполнение работ (пол-ль)","Ремонтные работы (пол-ль)","Уч ремонтов метал.оборудования (пол-ль)","Уч ремонтно-строительных работ (пол-ль)","Уч ремонта ДП1 (пол-ль)","Уч ремонтов электрооборудования цехов предприятия (пол-ль)")
"бу показателя"
